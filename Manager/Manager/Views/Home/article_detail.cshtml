
@{
    ViewBag.Title = "article_detail";
    Layout = null;
    var shows = (Manager.Models.news)Session["shows"];
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title></title>
    <style>
        #div1{
            margin-left:500px;
            width:200px;
        }
    </style>
    <script type="text/javascript">
	<link href="@Url.Content("~/css/bootstrap.css")" rel="stylesheet">
    <script src="@Url.Content("~/Script/bootstrap.js")"></script>
    <script type="text/javascript" src="~/Scripts/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div id="div1">
        <h2>@shows.title</h2>
        <h5>@shows.author</h5>
        <br /> 
        @Html.Raw(shows.contents);
    </div>
</body>
</html>


